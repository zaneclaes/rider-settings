<application>
  <component name="KeyPromoterXStatistic">
    <option name="statistics">
      <Statistic Action="Build">
        <Item>
          <IdeaActionID>ActivateBuildToolWindow</IdeaActionID>
          <ShortCut>'⌘0'</ShortCut>
          <Description>Build</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Delete...">
        <Item>
          <IdeaActionID>$Delete</IdeaActionID>
          <ShortCut>'⌦' or '⌫' or '⌘⌫'</ShortCut>
          <Description>Delete...</Description>
          <Count>4</Count>
        </Item>
      </Statistic>
      <Statistic Action="Errors In Solution">
        <Item>
          <IdeaActionID>ActivateErrorsInSolutionToolWindow</IdeaActionID>
          <ShortCut>'⌃⌘2'</ShortCut>
          <Description>Errors In Solution</Description>
          <Count>3</Count>
        </Item>
      </Statistic>
      <Statistic Action="Indent Selection">
        <Item>
          <IdeaActionID>EditorIndentSelection</IdeaActionID>
          <ShortCut>'⇥'</ShortCut>
          <Description>Indent Selection</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Properties...">
        <Item>
          <IdeaActionID>EditPropertiesAction</IdeaActionID>
          <ShortCut>'⌥⏎'</ShortCut>
          <Description>Properties...</Description>
          <Count>2</Count>
        </Item>
      </Statistic>
      <Statistic Action="Rename...">
        <Item>
          <IdeaActionID>RenameElement</IdeaActionID>
          <ShortCut>'⌘R, R' or '⌘R, ⌘R'</ShortCut>
          <Description>Rename...</Description>
          <Count>5</Count>
        </Item>
      </Statistic>
      <Statistic Action="Solution">
        <Item>
          <IdeaActionID>ActivateSolutionToolWindow</IdeaActionID>
          <ShortCut>'⌘+1'</ShortCut>
          <Description>Solution</Description>
          <Count>2</Count>
        </Item>
      </Statistic>
      <Statistic Action="Unindent Line or Selection">
        <Item>
          <IdeaActionID>EditorUnindentSelection</IdeaActionID>
          <ShortCut>'⇧⇥' or '⌘/'</ShortCut>
          <Description>Unindent Line or Selection</Description>
          <Count>2</Count>
        </Item>
      </Statistic>
      <Statistic Action="Unit Tests">
        <Item>
          <IdeaActionID>ActivateUnitTestsToolWindow</IdeaActionID>
          <ShortCut>'⌘8' or '⇧⌘8'</ShortCut>
          <Description>Unit Tests</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
    </option>
  </component>
</application>